{
  "ecmaFeatures": {
    "jsx": true,
    "modules":true
  },
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "rules": {
    "strict": 0,
    "semi":1,
    "eqeqeq":1,
    "guard-for-in":1,
    "no-else-return":1,
    "handle-callback-err":1,
    "no-underscore-dangle": 0,
    "quotes": [
      2,
      "single"
    ]
  }
}
